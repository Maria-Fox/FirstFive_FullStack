{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "New Project Root Schema",
    "required": [
        "name",
        "project_desc",
        "timeframe"
    ],
    "properties": {
        "name": {
            "type": "string",
            "default": "",
            "title": "The name Schema",
            "examples": [
                "Design a smart TV APP layout "
            ]
        },
        "project_desc": {
            "type": "string",
            "default": "",
            "title": "The project_desc Schema",
            "examples": [
                "Beautifully design an interface for smart TV platforms. Include comprehnsive resting for all devices."
            ]
        },
        "timeframe": {
            "type": "string",
            "default": "",
            "title": "The timeframe Schema",
            "examples": [
                "3-6 months"
            ]
        },
        "github_repo": {
            "type": "string",
            "default": "",
            "title": "The github_repo Schema",
            "examples": [
                "https://github.com/Maria-Fox/FirstFive"
            ]
        }
    },
    "examples": [{
        "name": "Design a smart TV APP layout ",
        "project_desc": "Beautifully design an interface for smart TV platforms. Include comprehnsive resting for all devices.",
        "timeframe": "3-6 months",
        "github_repo": "https://github.com/Maria-Fox/FirstFive"
    }]
}