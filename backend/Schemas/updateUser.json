{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Update profile Schema",
  "required": [
      "username"
  ],
  "properties": {
      "username": {
          "type": "string",
          "default": "",
          "title": "The username Schema",
          "examples": [
              "insomSunday"
          ]
      },
      "password": {
          "type": "string",
          "default": "",
          "title": "The password Schema",
          "examples": [
              "insomniaSunday"
          ]
      },
      "email": {
          "type": "string",
          "default": "",
          "title": "The email Schema",
          "examples": [
              "email@yikes.com"
          ]
      },
      "bio": {
          "type": "string",
          "default": "",
          "title": "The bio Schema",
          "examples": [
              "another bio"
          ]
      }
  },
  "examples": [{
      "username": "insomSunday",
      "password": "insomniaSunday",
      "email": "email@yikes.com",
      "bio": "another bio"
  }]
}