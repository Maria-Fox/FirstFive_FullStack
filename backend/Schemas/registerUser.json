{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Register new user Schema",
    "required": [
        "username",
        "password",
        "email",
        "bio"
    ],
    "properties": {
        "username": {
            "type": "string",
            "default": "",
            "title": "The username Schema",
            "examples": [
                "softwareDev1"
            ]
        },
        "password": {
            "type": "string",
            "default": "",
            "title": "The password Schema",
            "examples": [
                "pw12345667"
            ]
        },
        "email": {
            "type": "string",
            "default": "",
            "title": "The contact_email Schema",
            "examples": [
                "bestemail@aol.com"
            ]
        },
        "bio": {
            "type": "string",
            "default": "",
            "title": "The bio Schema",
            "examples": [
                "Best software dev in the area!"
            ]
        }
    },
    "examples": [{
        "username": "softwareDev1",
        "password": "pw12345667",
        "email": "bestemail@aol.com",
        "bio": "Best software dev in the area!"
    }]
}